        this.start();
        this.stop();
    &#125;
&#125;

package template;

public class Ocar extends Model &#123;
    @Override
    protected void start() &#123;

    &#125;

    @Override
    protected void stop() &#123;

    &#125;
&#125;

package template;

public class Wcar extends Model &#123;
    @Override
    protected void start() &#123;

    &#125;

    @Override
    protected void stop() &#123;

    &#125;
&#125;

package template;

public class Client &#123;
    public static void main(String[] args) &#123;
        Model wcar=new Wcar();
        wcar.excet();

        Model ocar=new Ocar();
        ocar.excet();
    &#125;

&#125;
</code></pre>
]]></content>
